def print_greeting():
    """Prints a "Hello, world!" greeting to the console."""
    print("Hello, world!")


def calculate_sum(num1: int, num2: int) -> int:
    """Calculates and returns the sum of two numbers.

    Args:
        num1: The first number.
        num2: The second number.

    Returns:
        The sum of the two numbers.
    """
    return num1 + num2


def print_numbers(*numbers: int):
    """Prints a sequence of numbers to the console, separated by spaces.

    Args:
        *numbers: A variable number of numbers to print.
    """
    print(*numbers)


def main():
    """Main entry point to run the script's logic."""
    print_greeting()
    total = calculate_sum(1, 2)
    print(total)

    print_numbers(1, 2, 3)


if __name__ == "__main__":
    main()